{
    "name": "G_1 C-style",
    "terms": [
        "(",
        ")",
        "not",
        "==",
        "<>",
        "<",
        "<=",
        ">",
        ">=",
        "+",
        "-",
        "or",
        "*",
        "/",
        "div",
        "mod",
        "and",
        "{",
        "}",
        ";",
        "=",
        "конст",
        "идент"
    ],
    "nonterms": [
        "выражение",
        "простое выражение",
        "простое выражение'",
        "оп отношения",
        "оп типа сложения",
        "оп типа умножения",
        "тттерм",
        "тттерм'",
        "знак",
        "фактор",
        "программа",
        "блок",
        "список операторов",
        "оператор",
        "хвост"
    ],
    "epsilon": "eps",
    "startSymbol": "программа",
    "prods": {
        "программа": [
            ["блок"]
        ],
        "блок": [
            ["{", "список операторов", "}"]
        ],
        "список операторов": [
            ["оператор", "хвост"],
            ["оператор"]
        ],
        "хвост": [
            [";", "оператор", "хвост"],
            [";", "оператор"]
        ],
        "оператор": [
            ["идент", "=", "выражение"],
            ["{", "список операторов", "}"]
        ],
        "выражение": [
            ["фактор", "тттерм'", "простое выражение'"],
            ["знак", "тттерм", "простое выражение'"],
            ["фактор", "тттерм'", "простое выражение'", "оп отношения", "простое выражение"],
            ["фактор"],
            ["фактор", "тттерм'", "оп отношения", "простое выражение"],
            ["фактор", "простое выражение'"],
            ["фактор", "оп отношения", "простое выражение"],
            ["фактор", "простое выражение'", "оп отношения", "простое выражение"],
            ["знак", "тттерм", "простое выражение'", "оп отношения", "простое выражение"],
            ["знак", "тттерм", "оп отношения", "простое выражение"],
            ["знак", "тттерм"],
            ["фактор", "тттерм'"]
        ],
        "простое выражение": [
            ["тттерм"],
            ["знак", "тттерм"],
            ["тттерм", "простое выражение'"],
            ["знак", "тттерм", "простое выражение'"]
        ],
        "простое выражение'": [
            ["оп типа сложения", "тттерм"],
            ["оп типа сложения", "тттерм", "простое выражение'"]
        ],
        "тттерм": [
            ["фактор"],
            ["фактор", "тттерм'"]
        ],
        "тттерм'": [
            ["оп типа умножения", "фактор", "тттерм'"],
            ["оп типа умножения", "фактор"]
        ],
        "фактор": [
            ["идент"],
            ["конст"],
            ["(", "простое выражение", ")"],
            ["not", "фактор"]
        ],
        "знак": [["+"], ["-"]],
        "оп отношения": [["=="], ["<>"], ["<"], ["<="], [">"], [">="]],
        "оп типа сложения": [["+"], ["-"], ["or"]],
        "оп типа умножения": [["*"], ["/"], ["div"], ["mod"], ["and"]]
    }
}
